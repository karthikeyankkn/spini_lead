<h1>Welcome to login page</h1>



<%= form_for :session, url: login_path do |l| %>
	<p>
		<%= l.label :email %><br>
		<%= l.email_field :email %>
	</p>
	<p>
		<%= l.label :password %><br>
		<%= l.password_field :password, autocomplete: "off" %>
	</p>
	<p>
		<%= l.submit "Login" %>
	</p>
<% end %>

<p>
	<%= link_to "Back to user page", users_path %>
</p>